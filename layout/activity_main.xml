<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/dhan10"
    tools:context="com.example.sabarishnandha.intelligentcheck.MainActivity"
    tools:layout_editor_absoluteY="89dp">

    <EditText
        android:id="@+id/editText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="60dp"
        android:layout_y="17dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Welcome To IBTS system"
        android:textColor="@android:color/background_light"
        android:textSize="40dp"
        tools:layout_editor_absoluteX="192dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="140dp"
        android:layout_height="59dp"
        android:layout_x="37dp"
        android:layout_y="169dp"
        android:text="@string/source"
        android:textColor="@android:color/background_light"
        android:textSize="40dp"
        tools:layout_editor_absoluteX="52dp"
        tools:layout_editor_absoluteY="133dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="210dp"
        android:layout_height="90dp"
        android:layout_x="36dp"
        android:layout_y="325dp"
        android:text="@string/Destination"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textSize="40dp"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="318dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="168dp"
        android:layout_height="48dp"
        android:layout_x="35dp"
        android:layout_y="471dp"
        android:text="@string/Date"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textSize="40dp"
        tools:layout_editor_absoluteX="62dp"
        tools:layout_editor_absoluteY="540dp" />

    <EditText
        android:id="@+id/Birthday"
        android:layout_width="294dp"
        android:layout_height="102dp"
        android:layout_x="285dp"
        android:layout_y="439dp"
        android:clickable="true"
        android:editable="false"
        android:ems="10"
        android:hint="click here
"
        android:inputType="textPersonName"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        tools:layout_editor_absoluteX="300dp"
        tools:layout_editor_absoluteY="488dp"
        android:textSize="40dp"/>

    <Button
        android:id="@+id/button"
        android:layout_width="194dp"
        android:layout_height="96dp"
        android:layout_x="189dp"
        android:layout_y="643dp"
        android:background="@drawable/rounded_edge"
        android:onClick="goclick"
        android:text="Go"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textSize="40dp"
        tools:layout_editor_absoluteX="204dp"
        tools:layout_editor_absoluteY="657dp"
        />

    <AutoCompleteTextView
        android:id="@+id/autoCompleteTextView2"
        android:layout_width="284dp"
        android:layout_height="83dp"
        android:layout_x="281dp"
        android:layout_y="168dp"
        android:textSize="40dp"
        tools:layout_editor_absoluteX="254dp"
        tools:layout_editor_absoluteY="172dp" />

    <AutoCompleteTextView
        android:id="@+id/autoCompleteTextView3"
        android:layout_width="290dp"
        android:layout_height="79dp"
        android:layout_x="287dp"
        android:layout_y="317dp"
        android:textSize="40dp"
        tools:layout_editor_absoluteX="313dp"
        tools:layout_editor_absoluteY="325dp" />

</AbsoluteLayout>
